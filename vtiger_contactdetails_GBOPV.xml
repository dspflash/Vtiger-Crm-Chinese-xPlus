<GroupBox cols="xs12 sm6 md4" Name="GBOPVvtiger_contactdetails" Size="100,100"><Ctrls>
<Button Text="delete" ToolTip="ttp_deletevtiger_contactdetails" Name="deletevtiger_contactdetails" Size="50,20">
	<OnClick>
	<DbDetails fcsm="1">
<DbTable>vtigercrm6.vtiger_contactdetails</DbTable>
<Where>contactid='%{vtiger_contactdetailsLst[contactid]}'</Where>
<queryFormat>delete from %{table} where %{where}</queryFormat></DbDetails>
	<submit namelist="DbDetails"/><submit next="searchvtiger_contactdetails" function="click"/>
	</OnClick>
	</Button><TAB/>
<Button Text="refresh" ToolTip="ttp_refreshvtiger_contactdetails" Name="searchvtiger_contactdetails" Size="50,20">
			  <OnClick><submit next="vtiger_contactdetailsLst"/>	</OnClick>
</Button>
<TAB/><TAB/><Button Text="firstpage" Name="FPvtiger_contactdetailsLst" OnClick="(vtiger_contactdetailsLst)GetPage(First)" Size="50,20"/>
<TAB/><Button Text="prepage" Name="PREvtiger_contactdetailsLst" OnClick="(vtiger_contactdetailsLst)GetPage(Pre)" Size="50,20"/>
<TAB/><Button Text="nextpage" Name="NPvtiger_contactdetailsLst" OnClick="(vtiger_contactdetailsLst)GetPage(Next)" Size="50,20"/>
</Ctrls></GroupBox>