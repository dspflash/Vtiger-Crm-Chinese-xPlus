<GroupBox Name="GBDVvtiger_potential" cols="xs12 sm12 md12 lg12" visible="0"><Ctrls>
<ListCtrl Name="vtiger_potentialLst" ToolTip="ttp_vtiger_potential" Size="70,20" visible="0" loadedclick="1">
