		<GroupBox cols="xs12 sm6 md4" Multiline="0" overlap="pre" Name="GB_projectid" Visible="false"><Ctrls>
		<Label Align="Right" Text="projectid" overlap="pre" Name="lb_projectid" readonly="true" Size="60,20" Visible="false"/>
