	<DbDetails>
<Index>accountid</Index>
<DbTable>vtigercrm6.vtiger_account</DbTable>
<Data>accountname,accountid</Data>
<Where/>
<queryFormat/></DbDetails><OnSelEndOk><submit next="referid"/></OnSelEndOk></ComboBox>
<MagicCtrl Name="refreshlinktoaccountscontacts" Link="tip:刷新" images=",data/refresh.png" button="type:0" Size="20,20"><OnClick><submit next="linktoaccountscontacts"/></OnClick></MagicCtrl>\r\n</Ctrls></GroupBox>
